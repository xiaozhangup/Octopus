diff --git a/src/main/java/org/bukkit/event/block/EndPortalCreateEvent.java b/src/main/java/org/bukkit/event/block/EndPortalCreateEvent.java
new file mode 100644
index 0000000000000000000000000000000000000000..e34c5182aaa916bd26f2b595792bc0fa86d5fa60
